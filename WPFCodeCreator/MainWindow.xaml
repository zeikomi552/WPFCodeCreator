<mah:MetroWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:local="clr-namespace:WPFCodeCreator"
        TitleCharacterCasing="Normal"
        xmlns:UserControls="clr-namespace:WPFCodeCreator.Views.UserControls" x:Class="WPFCodeCreator.MainWindow"
        mc:Ignorable="d"
        Title="WPFCodeCreator" Height="800" Width="1200">
    <Grid>
        <TabControl>
            <TabItem Header="プロパティ">
                <UserControls:ucPropertyV/>
            </TabItem>
            <TabItem Header="依存プロパティ">
                <UserControls:ucDependencyPropertyV/>
            </TabItem>
            <TabItem Header="コンバーター">
                <UserControls:ucConverterV/>
            </TabItem>
            <TabItem Header="アクション"/>
            <TabItem Header="ビヘイビア"/>
            
        </TabControl>
    </Grid>
</mah:MetroWindow>
